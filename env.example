# Environment Variables Template
# Copy to .env and fill in your values

# API Configuration
EXPO_PUBLIC_API_BASE_URL=https://api.citibank.com/v1
EXPO_PUBLIC_API_TIMEOUT=30000

# Environment
EXPO_PUBLIC_ENVIRONMENT=development

# Feature Flags
EXPO_PUBLIC_ENABLE_BIOMETRIC=true
EXPO_PUBLIC_ENABLE_INTERNATIONAL_TRANSFERS=true

# Security
EXPO_PUBLIC_ENCRYPTION_KEY=your-encryption-key-here

# Analytics (Optional)
EXPO_PUBLIC_ANALYTICS_ID=your-analytics-id

# Deep Linking
EXPO_PUBLIC_DEEP_LINK_SCHEME=citibank
