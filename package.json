{
  "name": "citi-mobile-banking",
  "version": "1.0.0",
  "description": "Enterprise-grade Citibank Mobile Banking Application",
  "main": "expo/AppEntry.js",
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "build:android": "eas build --platform android",
    "build:ios": "eas build --platform ios",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint . --ext .ts,.tsx,.js,.jsx",
    "lint:fix": "eslint . --ext .ts,.tsx,.js,.jsx --fix",
    "type-check": "tsc --noEmit",
    "format": "prettier --write \"**/*.{ts,tsx,js,jsx,json,md}\"",
    "prepare": "husky install",
    "storybook": "expo start --web --webpack-config ./storybook/webpack.config.js",
    "prestorybook": "sb-rn-get-stories",
    "security:audit": "npm audit",
    "security:check": "npm audit --audit-level moderate"
  },
  "dependencies": {
    "@expo/vector-icons": "^13.0.0",
    "@react-native-async-storage/async-storage": "1.18.2",
    "@react-native-community/netinfo": "9.3.10",
    "@react-navigation/bottom-tabs": "^6.5.8",
    "@react-navigation/native": "^6.1.18",
    "@react-navigation/native-stack": "^6.11.0",
    "@react-navigation/stack": "^6.3.20",
    "@reduxjs/toolkit": "^1.9.5",
    "@shopify/flash-list": "1.4.3",
    "expo": "~49.0.8",
    "expo-app-loading": "~2.1.0",
    "expo-auth-session": "~5.0.2",
    "expo-blur": "~12.4.1",
    "expo-constants": "~14.4.2",
    "expo-crypto": "~12.4.1",
    "expo-device": "~5.4.0",
    "expo-font": "~11.4.0",
    "expo-haptics": "~12.4.0",
    "expo-image": "~1.3.5",
    "expo-local-authentication": "~13.4.1",
    "expo-notifications": "~0.20.1",
    "expo-router": "^2.0.0",
    "expo-secure-store": "~12.3.1",
    "expo-splash-screen": "~0.20.5",
    "expo-status-bar": "~1.6.0",
    "expo-symbols": "^1.0.7",
    "expo-updates": "~0.18.12",
    "i18next": "^23.4.4",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-hook-form": "^7.45.4",
    "react-i18next": "^13.1.2",
    "react-native": "0.72.10",
    "react-native-gesture-handler": "~2.12.0",
    "react-native-keychain": "^8.1.3",
    "react-native-paper": "^5.9.1",
    "react-native-reanimated": "~3.3.0",
    "react-native-safe-area-context": "^4.6.3",
    "react-native-screens": "~3.22.0",
    "react-native-svg": "^15.13.0",
    "react-native-web": "~0.19.6",
    "react-query": "^3.39.3",
    "react-redux": "^8.1.2",
    "redux-persist": "^6.0.0",
    "yup": "^1.2.0",
    "zustand": "^4.4.1"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@babel/plugin-proposal-export-namespace-from": "^7.18.9",
    "@babel/runtime": "^7.22.6",
    "@commitlint/cli": "^17.6.7",
    "@commitlint/config-conventional": "^17.6.7",
    "@expo/webpack-config": "^19.0.0",
    "@storybook/addon-actions": "^6.5.16",
    "@storybook/addon-essentials": "^6.5.16",
    "@storybook/addon-react-native-web": "^0.0.19",
    "@storybook/react": "^6.5.16",
    "@testing-library/jest-native": "^5.4.2",
    "@testing-library/react-native": "^12.1.3",
    "@types/jest": "^29.5.3",
    "@types/react": "~18.2.0",
    "@typescript-eslint/eslint-plugin": "^6.2.1",
    "@typescript-eslint/parser": "^6.2.1",
    "babel-loader": "^8.3.0",
    "babel-plugin-module-resolver": "^5.0.2",
    "detox": "^20.10.2",
    "eslint": "^8.45.0",
    "eslint-config-expo": "^7.0.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-react": "^7.33.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-native": "^4.0.0",
    "husky": "^8.0.3",
    "jest": "^29.2.1",
    "jest-expo": "~49.0.0",
    "lint-staged": "^13.2.3",
    "prettier": "^3.0.0",
    "react-test-renderer": "18.2.0",
    "typescript": "^5.1.6"
  },
  "lint-staged": {
    "*.{ts,tsx,js,jsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,md}": [
      "prettier --write"
    ]
  },
  "private": true,
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  }
}
